
CC=cl.exe
LD=link.exe
coreCFLAGS= /c /O2 /Ot /Za /Tc
CFLAGS=/DNONE
NAME=addAmr.exe
PREFIX="%localAppData%"
O=o.win

genAmr=..\genAmr
genLib=..\genLib
genBio=..\genBio

objFiles= \
   $(genAmr)\addAmr.$O \
   $(genAmr)\amrST.$O \
   $(genAmr)\drugAry.$O \
   $(genLib)\base10str.$O \
   $(genLib)\fileFun.$O \
   $(genLib)\ulCp.$O \
   $(genBio)\seqST.$O \
   $(genBio)\geneCoord.$O \
   $(genBio)\codonFun.$O \
   $(genLib)\genMath.$O \
   mainAddAmr.$O

$(NAME): mainAddAmr.$O
	$(LD) /out:$(NAME) $(objFiles)

mainAddAmr.$O: \
	mainAddAmr.c \
	$(genAmr)\addAmr.$O \
	../genLib/endLine.h \
	../bioTools.h
		$(CC) /Fo:mainAddAmr.$O \
			$(CFLAGS) $(coreCFLAGS) mainAddAmr.c

$(genAmr)\addAmr.$O: \
	$(genAmr)\addAmr.c \
	$(genAmr)\addAmr.h \
	$(genAmr)\amrST.$O \
	$(genBio)\seqST.$O \
	$(genBio)\geneCoord.$O \
	$(genBio)\codonFun.$O \
	$(genLib)\genMath.$O \
	$(genBio)\ntTo2Bit.h \
	$(genBio)\revNtTo2Bit.h \
	$(genBio)\codonTbl.h
		$(CC) /Fo:$(genAmr)\addAmr.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genAmr)\addAmr.c


$(genAmr)\amrST.$O: \
	$(genAmr)\amrST.c \
	$(genAmr)\amrST.h \
	$(genAmr)\drugAry.$O \
	$(genLib)\fileFun.$O \
	$(genLib)\base10str.$O \
	$(genLib)\64bit.h \
	$(genLib)\endLine.h
		$(CC) /Fo:$(genAmr)\amrST.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genAmr)\amrST.c


$(genAmr)\drugAry.$O: \
	$(genAmr)\drugAry.c \
	$(genAmr)\drugAry.h
		$(CC) /Fo:$(genAmr)\drugAry.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genAmr)\drugAry.c


$(genLib)\base10str.$O: \
	$(genLib)\base10str.c \
	$(genLib)\base10str.h
		$(CC) /Fo:$(genLib)\base10str.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genLib)\base10str.c

$(genLib)\fileFun.$O: \
	$(genLib)\fileFun.c \
	$(genLib)\fileFun.h \
	$(genLib)\ulCp.$O
		$(CC) /Fo:$(genLib)\fileFun.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genLib)\fileFun.c

$(genLib)\ulCp.$O: \
	$(genLib)\ulCp.c \
	$(genLib)\ulCp.h
		$(CC) /Fo:$(genLib)\ulCp.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genLib)\ulCp.c

$(genBio)\seqST.$O: \
	$(genBio)\seqST.c \
	$(genBio)\seqST.h \
	$(genLib)\fileFun.$O
		$(CC) /Fo:$(genBio)\seqST.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genBio)\seqST.c

$(genBio)\geneCoord.$O: \
	$(genBio)\geneCoord.c \
	$(genBio)\geneCoord.h \
	$(genLib)\fileFun.$O \
	$(genLib)\base10str.$O \
	$(genLib)\ulCp.$O \
	$(genLib)\genMath.h
	$(CC) /Fo:$(genBio)\geneCoord.$O \
		$(CFLAGS) $(coreCFLAGS) \
		$(genBio)\geneCoord.c

$(genBio)\codonFun.$O: \
	$(genBio)\codonFun.c \
	$(genBio)\codonFun.h \
	$(genLib)\ulCp.h \
	$(genBio)\codonTbl.h \
	$(genBio)\ntTo2Bit.h \
	$(genBio)\revNtTo2Bit.h
		$(CC) /Fo:$(genBio)\codonFun.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genBio)\codonFun.c

$(genLib)\genMath.$O: \
	$(genLib)\genMath.c \
	$(genLib)\genMath.h
		$(CC) /Fo:$(genLib)\genMath.$O \
			$(CFLAGS) $(coreCFLAGS) \
			$(genLib)\genMath.c

install:
	move $(NAME) $(PREFIX)

clean:
	del $(objFiles)
