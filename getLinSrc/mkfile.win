
CC=cl.exe
LD=link.exe
coreCFLAGS= /c /O2 /Ot /Za /Tc
CFLAGS=/DNONE
NAME=getLin.exe
PREFIX="%localAppData%"
O=o.win

genGeneoType=..\genGeneoType
genLib=..\genLib
genAln=..\genAln
genBio=..\genBio

