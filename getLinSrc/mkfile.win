   mainGetLin.$O

$(NAME): mainGetLin.$O
	$(LD) /out:$(NAME) $(objFiles)

mainGetLin.$O: \
	mainGetLin.c \
	$(genGeneoType)\getLin.$O \
	../bioTools.h
		$(CC) /Fo:mainGetLin.$O \
			$(CFLAGS) $(coreCFLAGS) mainGetLin.c

